<section id="exercises" class="exercises">
      <div class="container">
        <div class="section-title">
           <h3 class="exercise-name">{{exercise.exercise_name}}</h3> 
           
         </div>
       
        <div class="row">
          <div class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="fade-up">
          <div>
           
          <img width="360" height="480" src="/images/exercises/{{exercise.exercise_type}}/{{exercise.exercise_path}}/{{exercise.img_start}}" data-toggle="tooltip" data-placement="top" title="Starting Position">
          <h5 class="card-title">Starting Position</h5>
          </div>
          </div>
          <div class="col-lg-4 col-md-6 d-flex align-items-stretch mt-4 mt-md-0" data-aos="fade-up" data-aos-delay="150">
            <div>
            <img width="360" height="480" src="/images/exercises/{{exercise.exercise_type}}/{{exercise.exercise_path}}/{{exercise.img_end}}" data-toggle="tooltip" data-placement="top" title="Ending Position">
      
            <h5 class="card-title">Ending Position</h5>
            </div>
          </div>      
        </div>
      </div>
    </section>
    <section>
             <button type="button" class="btn btn-info del-exercise" id={{id}}>Delete</button>
    </section>



